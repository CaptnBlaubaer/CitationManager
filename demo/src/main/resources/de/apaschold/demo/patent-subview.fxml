<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="10.0" style="-fx-padding: 10px;" xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="de.apaschold.demo.gui.citationdetailsview.PatentSubViewController">
    <Label text="Title"/>
    <TextField fx:id="titleChange" style="-fx-font-size: 12;"/>
    <Label text="Author(s)"/>
    <TextArea fx:id="authorsChange" style="-fx-font-size: 12;"/>
    <Label text="Year"/>
    <TextField fx:id="yearChange" style="-fx-font-size: 12;"/>
    <Label text="URL"/>
    <TextField fx:id="urlChange" style="-fx-font-size: 12;"/>
    <Button text="Save Changes" onAction="#saveChanges"/>
</VBox>


